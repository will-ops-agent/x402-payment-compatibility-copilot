# Copy to terraform.tfvars and fill in your values:
#   cp terraform.tfvars.example terraform.tfvars

# ─── Required ──────────────────────────────────────────────────────
function_name  = ""   # Unique name for this agent (e.g. "my-agent")
wallet_address = ""   # Your Ethereum wallet (e.g. "0x1234...")
private_key    = ""   # Private key for signing (e.g. "0xabcd...")

# ─── Network ───────────────────────────────────────────────────────
# Base Sepolia (testnet) — start here
network = "eip155:84532"
rpc_url = "https://sepolia.base.org"

# Base Mainnet — uncomment when ready for production
# network = "eip155:8453"
# rpc_url = "https://mainnet.base.org"

